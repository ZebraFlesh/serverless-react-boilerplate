{
  "name": "serverless-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "sls offline start --exec \"webpack-dev-server --config webpack.client.config.js --mode development\"",
    "build": "sls package",
    "build:client": "webpack --config webpack.client.config.js --mode production",
    "deploy": "sls deploy",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "aws-sdk": "^2.464.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "null-loader": "^2.0.0",
    "regenerator-runtime": "^0.13.2",
    "serverless": "^1.44.1",
    "serverless-export-env": "^1.2.0",
    "serverless-offline": "^4.10.6",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-s3-deploy": "^0.8.1",
    "serverless-webpack": "^5.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-stats-plugin": "^0.2.1"
  }
}
